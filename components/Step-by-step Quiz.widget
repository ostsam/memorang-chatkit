{
  "version": "1.0",
  "name": "Step-by-step Quiz (copy)",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{%- set _c -%},{\"type\":\"Row\",\"align\":\"center\",\"children\":[{\"type\":\"Icon\",\"name\":\"notebook-pencil\",\"size\":\"lg\"},{\"type\":\"Text\",\"value\":{{ (header_label) | tojson }},\"size\":\"sm\",\"color\":\"secondary\"},{\"type\":\"Spacer\"},{\"type\":\"Badge\",\"label\":{{ (badge_label) | tojson }}}]}{%-if mode === \"intro\" -%},{\"type\":\"Col\",\"gap\":2,\"children\":[{\"type\":\"Title\",\"value\":{{ (lesson.title) | tojson }},\"size\":\"md\"},{\"type\":\"Text\",\"value\":{{ (lesson.source) | tojson }},\"color\":\"secondary\"},{\"type\":\"Text\",\"value\":{{ (lesson.description) | tojson }},\"size\":\"sm\"}]}{%-else-%}{%-endif-%}{%-if ((mode == \"question\") and current_question) -%},{\"type\":\"Col\",\"gap\":3,\"children\":[{%- set _c -%},{\"type\":\"Caption\",\"value\":{{ ((\"Question \" ~ (progress.index + 1) ~ \" of \" ~ progress.total)) | tojson }}},{\"type\":\"Title\",\"value\":{{ (current_question.question) | tojson }},\"size\":\"sm\"},{\"type\":\"RadioGroup\",\"name\":\"quiz.current.choice\",\"ariaLabel\":\"Choices\",\"options\":{{ (option_list) | tojson }},\"defaultValue\":{{ (current_answer_value) | tojson }},\"direction\":\"col\",\"onChangeAction\":{\"type\":\"quiz.answer\",\"payload\":{\"questionId\":{{ (current_question.id) | tojson }}}},\"disabled\":{{ (view_locked) | tojson }}}{%-if show_hint -%},{\"type\":\"Row\",\"gap\":2,\"align\":\"start\",\"children\":[{\"type\":\"Box\",\"background\":\"red-100\",\"radius\":\"sm\",\"padding\":2,\"children\":[{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Icon\",\"name\":\"info\",\"color\":\"danger\"},{\"type\":\"Text\",\"value\":{{ (current_question.hint) | tojson }},\"size\":\"sm\",\"color\":\"danger\"}]}]}]}{%-else-%}{%-endif-%}{%-if show_explanation -%},{\"type\":\"Row\",\"gap\":2,\"align\":\"start\",\"children\":[{\"type\":\"Box\",\"background\":\"green-400\",\"radius\":\"sm\",\"padding\":2,\"children\":[{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Icon\",\"name\":\"check-circle\",\"color\":\"white\"},{\"type\":\"Text\",\"value\":{{ (current_question.explanation) | tojson }},\"size\":\"sm\",\"color\":\"white\"}]}]}]}{%-else-%}{%-endif-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}{%-else-%}{%-endif-%}{%-if mode === \"summary\" -%},{\"type\":\"Col\",\"gap\":2,\"align\":\"start\",\"children\":[{\"type\":\"Title\",\"value\":\"All done!\",\"size\":\"md\"},{\"type\":\"Text\",\"value\":{{ ((\"Score: \" ~ score.correct ~ \" / \" ~ score.total)) | tojson }}},{\"type\":\"Caption\",\"value\":\"Use Restart to try again.\",\"color\":\"secondary\"}]}{%-else-%}{%-endif-%},{\"type\":\"Divider\"}{%-if mode !== \"summary\" -%},{\"type\":\"Row\",\"children\":[{\"type\":\"Button\",\"label\":\"Back\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"quiz.prev\"},\"disabled\":{% if not (controls.can_back) %}\"true\"{% else %}{{ (undefined) | tojson }}{% endif %}},{\"type\":\"Spacer\"},{\"type\":\"Button\",\"label\":{{ (controls.next_label) | tojson }},\"style\":\"primary\",\"onClickAction\":{\"type\":\"quiz.next\"},\"disabled\":{% if not (controls.can_next) %}\"true\"{% else %}{{ (undefined) | tojson }}{% endif %}}]}{%-else-%},{\"type\":\"Row\",\"children\":[{\"type\":\"Button\",\"label\":\"Restart\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"quiz.restart\"}},{\"type\":\"Spacer\"},{\"type\":\"Button\",\"label\":\"Close\",\"style\":\"primary\",\"onClickAction\":{\"type\":\"quiz.close\"}}]}{%-endif-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "lesson": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "source": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "questions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "number"
                },
                "question": {
                  "type": "string"
                },
                "choices": {
                  "minItems": 5,
                  "maxItems": 5,
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "number"
                      },
                      "label": {
                        "type": "string"
                      }
