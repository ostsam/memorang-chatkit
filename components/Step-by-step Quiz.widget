        "type": "string"
      },
      "badge_label": {
        "type": "string"
      },
      "current_question": {
        "anyOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "number"
              },
              "question": {
                "type": "string"
              },
              "choices": {
                "minItems": 5,
                "maxItems": 5,
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "number"
                    },
                    "label": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "id",
                    "label"
                  ],
                  "additionalProperties": false
                }
              },
              "hint": {
                "type": "string"
              },
              "explanation": {
                "type": "string"
              },
              "correct_choice_id": {
                "type": "number"
              }
            },
            "required": [
              "id",
              "question",
              "choices",
              "hint",
              "explanation",
              "correct_choice_id"
            ],
            "additionalProperties": false
          },
          {
            "type": "null"
          }
        ]
      },
      "option_list": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string"
            },
            "value": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          },
          "required": [
            "label",
            "value"
          ],
          "additionalProperties": false
        }
      },
      "current_answer_value": {
        "type": "string"
      },
      "answers": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "question_id": {
              "type": "number"
            },
            "selected_choice_id": {
              "anyOf": [
                {
                  "type": "number"
                },
                {
                  "type": "null"
                }
              ]
            },
            "is_correct": {
              "type": "boolean"
            },
            "attempted": {
              "type": "boolean"
            }
          },
          "required": [
            "question_id",
            "selected_choice_id",
            "is_correct",
            "attempted"
          ],
          "additionalProperties": false
        }
      },
      "view_locked": {
        "type": "boolean"
      },
      "show_hint": {
        "type": "boolean"
      },
      "show_explanation": {
        "type": "boolean"
      },
      "controls": {
        "type": "object",
        "properties": {
          "can_back": {
            "type": "boolean"
          },
          "can_next": {
            "type": "boolean"
          },
          "next_label": {
            "type": "string"
          }
        },
        "required": [
          "can_back",
          "can_next",
          "next_label"
        ],
        "additionalProperties": false
      },
      "score": {
        "type": "object",
        "properties": {
          "correct": {
            "type": "number"
          },
          "total": {
            "type": "number"
          }
        },
        "required": [
          "correct",
          "total"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "lesson",
      "mode",
      "current_page",
      "progress",
      "header_label",
      "badge_label",
      "current_question",
      "option_list",
      "answers",
      "view_locked",
      "show_hint",
      "show_explanation",
      "controls",
      "score"
    ],
    "additionalProperties": false
  },
